{
  "name": "netlist-visualizer-frontend-service",
  "version": "1.0.0",
  "description": "Frontend for netlist visualizer",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server/index.js",
    "compile": "ts-node build.ts && tsc && npm run prod:client && node ./update-views.js",
    "dev": "NODE_ENV=development npx concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack --config webpack.client.config.js --watch --env development",
    "dev:debug": "node ./update-views.js && nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "dev:server": "node ./update-views.js && nodemon server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "prod:client": "webpack --config webpack.client.config.js --env production",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@mantine/core": "^8.0.0",
    "@mantine/form": "^8.0.0",
    "@mantine/hooks": "^8.0.0",
    "@xyflow/react": "^12.6.0",
    "axios": "^1.9.0",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.13.8",
    "fs-extra": "^11.3.0",
    "hbs": "^4.2.0",
    "install": "^0.13.0",
    "npm": "^11.3.0",
    "pino": "^7.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/pino": "^7.0.4",
    "@types/react-dom": "^19.1.3",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.21",
    "chai": "^4.3.6",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.5.1",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.7.0",
    "typescript": "^5.0.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
